{
  "name": "Quotes4You",
  "description": "Have you ever wanted to get a daily quote personalized to your selected mood?",
  "version": "1.0",
  "manifest_version": 3,
  "author": "Elisha White",
  "action": {
    "default_locale": "en",
    "default_icon": {
      "19": "5_iconPicture19.png", 
      "38": "6_iconPicture38.png"
    }, 
    "default_popup": "extension.html",
    "default_title": "Have a good day",
    "icons": {
      "16": "4_iconPicture16.png",
      "48": "7_iconPicture48.png", 
      "128": "8_iconPicture128.png"
    }
  },
  "chrome_url_overrides" : {  "newtab": "extension.html"},
  "permissions": ["activeTab"]
}
